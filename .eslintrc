{
  "extends": ["@nuxtjs/eslint-config-typescript"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "vue/no-v-html": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" },
    ],
    "vue/multi-word-component-names": "off",
  },
}
